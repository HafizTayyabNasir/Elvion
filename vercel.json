{
  "version": 2,
  "builds": [
    {
      "src": "elvion_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "elvion_project/wsgi.py"
    }
  ],
  "env": {
    "DATABASE_URL": "@postgres_url",
    "POSTGRES_URL": "@postgres_url",
    "SECRET_KEY": "@secret_key",
    "GEMINI_API_KEY": "@gemini_api_key"
  }
}